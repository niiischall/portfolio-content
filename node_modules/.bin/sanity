else
  export NODE_PATH="/Users/nischalnikit/Desktop/work/Extras/portfolio/portfolio-content/node_modules/.pnpm/sanity@3.18.1_@types+react@18.2.31_react-dom@18.2.0_react@18.2.0_styled-components@5.3.11/node_modules/sanity/bin/node_modules:/Users/nischalnikit/Desktop/work/Extras/portfolio/portfolio-content/node_modules/.pnpm/sanity@3.18.1_@types+react@18.2.31_react-dom@18.2.0_react@18.2.0_styled-components@5.3.11/node_modules/sanity/node_modules:/Users/nischalnikit/Desktop/work/Extras/portfolio/portfolio-content/node_modules/.pnpm/sanity@3.18.1_@types+react@18.2.31_react-dom@18.2.0_react@18.2.0_styled-components@5.3.11/node_modules:/Users/nischalnikit/Desktop/work/Extras/portfolio/portfolio-content/node_modules/.pnpm/node_modules:$NODE_PATH"
